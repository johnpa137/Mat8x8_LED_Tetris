module piece_placer #() ();
	// adder values
	output logic [3:0][XSIZE-1:0] sumX; 
	input logic [3:0][XSIZE-1:0] aX, bX;
	output logic [3:0][YSIZE-1:0] sumY; 
	input logic [3:0][YSIZE-1:0] aY, bY;
	
	genvar i;
	generate
		for(i = 0; i < 4; i++) begin : adders
			adder #(.WIDTH(XSIZE)) xAdder (.sum(sumX[i]), .overflow( ), .a(aX[i]), .b(bX[i]), .cin( ));
			adder #(.WIDTH(YSIZE)) yAdder (.sum(sumY[i]), .overflow( ), .a(aY[i]), .b(bY[i]), .cin( ));
		end
	endgenerate
endmodule 